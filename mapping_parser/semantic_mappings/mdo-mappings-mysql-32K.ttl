    rr:tableName  "BandGap32K".

<FormationEnergy_MySQL_Source_1>
    rml:source <MP_DB>;
    rml:referenceFormulation ql:CSV;
    rr:tableName  "FormationEnergy32K".



<Calculation_MySQL_Source_2>
    rml:source <OQMD_DB>;
    rml:referenceFormulation ql:CSV;
    rr:tableName  "Calculation32K".

<Structure_MySQL_Source_2>
    rml:source <OQMD_DB>;
